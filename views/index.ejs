<!--listing files-->
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>File Manager</title>
    <link rel="stylesheet" href="/styles.css">
</head>
<body>
    <h1>File Manager</h1>
    <ul>
        <% files.forEach(file => { %>
            <li><a href="/files/<%= file %>"><%= file %></a></li>
        <% }); %>
    </ul>
</body>
</html>
